{
	"name": "collabri",
	"displayName": "collabri",
	"description": "",
	"version": "0.0.1",
	"publisher": "",
	"repository": "",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "change-explorer",
					"title": "Change Explorer",
					"icon": "media/icon.svg"
				}
			]
		},
		"views": {
			"change-explorer": [
				{
					"id": "contributors",
					"name": "Contributors"
				}
			]
		},
		"commands": [
			{
				"command": "procurrently.checkoutBranch",
				"title": "Procurrently: Checkout Branch"
			},
			{
				"command": "procurrently.toggleRemoteChanges",
				"title": "Procurrently: Toggle remote changes",
				"icon": {
					"light": "media/disable_sync.svg",
					"dark": "media/disable_sync_dark.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "procurrently.toggleRemoteChanges",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "jest --coverage"
	},
	"devDependencies": {
		"@types/jest": "^24.0.11",
		"@types/node": "^11.13.0",
		"jest": "^24.6.0",
		"ts-jest": "^24.0.1",
		"tslint": "^5.11.0",
		"typescript": "^3.4.1",
		"vscode": "^1.1.22"
	},
	"dependencies": {
		"@atom/teletype-crdt": "^0.9.0",
		"rimraf": "^2.6.3",
		"vscode-set-text": "^1.0.0"
	},
	"jest": {
		"collectCoverage": true,
		"runInBand": true
	}
}
